{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "functions": {
        "api/og.js": {
            "runtime": "@vercel/edge"
        }
    },
    "headers": [
        {
            "source": "/api/og",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=60, s-maxage=60"
                }
            ]
        }
    ]
}